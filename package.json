    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
