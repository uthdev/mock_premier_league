{
  "name": "cloud_backup",
  "version": "1.0.0",
  "description": "An image cloud backup server",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "dev": "ts-node-dev src/index.ts",
    "user:seed": "ts-node src/db/seeds/user.seed.ts",
    "test": "nyc --reporter=html --reporter=text mocha -r ts-node/register tests/**/*.spec.ts --exit"
  },
  "keywords": [],
  "author": "Uthdev",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.omit": "^4.5.6",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.8.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.3.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash.omit": "^4.5.0",
    "mongodb": "^4.3.1",
    "redis": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.42"
  }
}
